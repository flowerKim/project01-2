<style>
#loginDiv {
  text-align: right;
}
#paging {
  text-align: center;
}
#write {
  text-align: right;
}
.container1 {
  text-align: center;
  float: center;
  width: 550px;
  margin: 20px 25px 100px -0px; 
}
</style>
<body>
<div class='container1'> <!-- #main -->
<div id='listDiv'><!-- listDiv -->
	<h1 align='center'>Free Board</h1>
	<table id='listTable_free' class='table table-hover'>
	  <thead>  
		  <tr>
		    <th>No.</th>
		    <th>MembNo.</th>
		    <th>Name</th>
		    <th>Title</th>
		    <th>Hit</th>
		    <th>CreateDate</th>
		  </tr>
	  </thead>
	  <tbody></tbody>
	</table>
	<div id='loginDiv'>
  <a id='f_writeBtn' href='#' class='btn btn-primary btn-xs'>Write</a>
 </div>
	<div id='paging'>
	  <div><a id='prevBtn' href='#' class="btn btn-default btn-sm" >Prev</a>
	    <span id='pageNo'></span>
	    <a id='nextBtn' href='#' class="btn btn-default btn-sm" >Next</a>
	  </div>
	</div>
</div><!-- listDiv -->
</div><!-- #main -->
<script id="template_free" type="text/x-handlebars-template">
{{#each data}}
<tr class="data-row">
  <td>{{no}}</td>
  <td>{{mno}}</td>
  <td>{{name}}</td>
  <td><a class="nameLink" no="{{no}}" href="#">{{title}}</a></td>
  <td>{{hit}}</td>
  <td>{{createDate}}</td>
</tr>
{{/each}}
</script>

<script>
requirejs(['app/content_04'], function(content_04) {
  content_04.init();
  content_04.listBoard(1, 10);
  
});
</script>



</body></html>