        */
      }).on('fileuploaddone', function(e, data) {
        console.log(data.result);
        $('#files').html('');
        $.each(data.result.data, function (index, file) {
            $('<img>')
              .attr('src', file.url)
              .css('width', '100px')
              .appendTo('#files');
            $('<span/>')
            .text(file.name 
                + '(' + file.originName + ')'
                + ', ' + file.size)
                .appendTo('#files');
            $('#fPhoto').val(file.name);
        });
      }).on('fileuploadprogressall', function (e, data) {
