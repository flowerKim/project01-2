		memberInfo: function() {
			var moduleObj = this;
			$.getJSON(contextRoot + '/bitin/auth/loginInfo.do', function(result) {
				var data = result.data;
				if (result.state == 'yes') {
					console.log(data.no + " + " + data.name);
					$('#myid').val(data.no);
