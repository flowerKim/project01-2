<div class='container2'>
<div id='formDiv'>
<h2>Write</h2>
	<form class="form-horizontal">
	
	<div class="form-group">
      <label for="rNo" class="col-sm-2 control-label">MemberNo.</label>
      <div class="col-sm-8">
        <input type="text" class="form-control" id="rNo">
      </div>
    </div>  
  
    <div class="form-group">
      <label for="rTitle" class="col-sm-2 control-label">Title</label>
      <div class="col-sm-8">
        <input type="text" class="form-control" id="rTitle">
      </div>
    </div>
    
     <div class="form-group">
      <label for="rContent" class="col-sm-2 control-label">Content</label>
      <div class="col-sm-8">
        <textarea class="form-control" id="rContent" rows='8' style="resize:none;"></textarea>
      </div>
    </div>
    
    <div class="form-group">
      <label for="rWebAddress" class="col-sm-2 control-label">WebAddress</label>
      <div class="col-sm-8">
        <input type="text" class="form-control" id="rWebAddress">
      </div>
    </div>
  
    <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button id='rinsertBtn' type='button' class="btn btn-default btn-sm my-view">Write</button>
      <button id='rcancelBtn' type='button' class="btn btn-default btn-sm my-view">Cancel</button>
    </div>
  </div>
</form>
