@CHARSET "UTF-8";
html[lang=ko-KR] body {
  font-family: "Lucida Grande", "Lucida Sans Unicode", "Dotum", "Helvetica", "Arial", "Verdana", "sans-serif";
  font-size: small;
}
::selection {
    background:#5bb6c3;
    color: #FFF;
    text-shadow: none;
}
::-moz-selection {
    background:#5bb6c3;
    color: #FFF;
    text-shadow: none;
}
::-webkit-selection {
    background:#5bb6c3;
    color: #FFF;
    text-shadow: none;
}

#header-table {
	margin: 0px;
	padding: 0px;
	height: 58px;
	border-collapse: collapse;
	border-spacing: 0px;
	
}
#pageFooter {
	height: 40px;
	padding-top: 7px;
  background-color: #f0f0f0;
}

#paging {
  width: 90%;
  margin: 0 auto;
}

.my-message-warning {
  color: red;
}
.my-message-success {
  color: blue;
}